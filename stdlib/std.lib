(define nil (quote ()))

(define nil? (lambda (x) (= nil x)))

(define identity (lambda (x) x))

(define list (lambda (... args) args))

(define cond (macro (x ... rest)
  (syntax-quote (if (unquote (car x))
       (unquote (car (cdr x)))
        (if (unquote (nil? rest))
          ()
          (cond (unquote-splice rest)))))))

(define car'
  (lambda (arrs)
  (if (nil? arrs)
    nil
    (cons
      (car (car arrs))
      (recur (cdr arrs))))))

(define and
  (macro (x ... args)
    (if (nil? args)
      x
      (syntax-quote (if (unquote x) (and (unquote-splice args)) false)))))

(define or
  (macro (x ... args)
    (if (nil? args)
      x
      (syntax-quote (if (unquote x) true (or (unquote-splice args)))))))

(define has-nil?
  (lambda (arr)
  (if (nil? arr)
    false
    (or (nil? (car arr)) (recur (cdr arr))))))
